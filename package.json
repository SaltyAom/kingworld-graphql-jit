  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.11",
    "@types/json-schema": "^7.0.11",
    "@types/lodash.memoize": "^4.1.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.mergewith": "^4.6.7",
    "@types/node": "^18.11.7",
